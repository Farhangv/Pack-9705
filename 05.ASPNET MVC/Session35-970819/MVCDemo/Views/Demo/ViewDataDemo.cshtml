@using MVCDemo.Models
<div class="alert alert-success">
    @ViewData["Message"]
</div>

<hr/>
<table class="table table-striped">
    <thead>
        <tr>
            <th>نام</th>
            <th>فامیل</th>
            <th>سن</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in ((List<Person>)ViewData["Users"]))
        {
        <tr>
            <td>@user.Name</td>
            <td>@user.Family</td>
            <td>@user.Age</td>
        </tr>
        }
    </tbody>
</table>
